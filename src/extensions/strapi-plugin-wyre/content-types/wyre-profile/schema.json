{
  "kind": "collectionType",
  "collectionName": "wyre_profiles",
  "info": {
    "singularName": "wyre-profile",
    "pluralName": "wyre-profiles",
    "displayName": "Wyre profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "wyreUUID": {
      "type": "string",
      "unique": true,
      "minLength": 3
    },
    "email": {
      "type": "email",
      "required": true
    },
    "givenName": {
      "type": "string",
      "required": true
    },
    "familyName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "dateOfBirth": {
      "type": "string",
      "required": true
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-plugin-wyre.address"
    },
    "debitCards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-plugin-wyre.debit-card",
      "private": true
    },
    "bankAccount": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::strapi-plugin-wyre.bank-account"
    }
  }
}
