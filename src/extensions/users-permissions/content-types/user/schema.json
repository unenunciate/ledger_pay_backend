{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "stytchUUID": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "required": false
    },
    "phone": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "wallets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contract.contract",
      "mappedBy": "user"
    },
    "wyre": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::strapi-plugin-wyre.wyre-profile",
      "inversedBy": "user"
    },
    "birthday": {
      "type": "date"
    },
    "discoverable": {
      "type": "boolean",
      "default": true
    },
    "worldcoinSetup": {
      "type": "boolean",
      "default": false
    },
    "EOA": {
      "type": "string"
    }
  }
}
